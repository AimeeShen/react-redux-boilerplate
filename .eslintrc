{
    "extends": "airbnb",
    "rules": {
        "comma-dangle": [2, "never"],
        "object-shorthand": 0,
        "indent": [2, 4],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/prop-types": 0,
        "react/sort-comp": 0,
        'react/jsx-boolean-value': [2, 'always']
    },
     "globals": {
        "ga": true
    }
}